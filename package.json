{
  "name": "css-in-js-benchmarks",
  "version": "1.0.0",
  "description": "Benchmarking CSS-in-JS libraries",
  "main": "index.js",
  "scripts": {
    "build-aphrodite": "webpack --config webpack-config/aphrodite.config.js --json=results/build-stats/aphrodite.stats.json",
    "build-emotion": "webpack --config webpack-config/emotion.config.js --json=results/build-stats/emotion.stats.json",
    "build-react-jss": "webpack --config webpack-config/react-jss.config.js --json=results/build-stats/react-jss.stats.json",
    "build-sass": "webpack --config webpack-config/sass.config.js --json=results/build-stats/sass.stats.json",
    "build-styled-components": "webpack --config webpack-config/styled-components.config.js --json=results/build-stats/styled-components.stats.json",
    "build-typestyle": "webpack --config webpack-config/typestyle.config.js --json=results/build-stats/typestyle.stats.json",
    "number-bench": "node test-suite/number.requests.test.js",
    "puppeteer-bench": "node test-suite/puppeteer.pageload.test.js",
    "build-all": "npm run build-react-jss && npm run build-sass && npm run build-styled-components && npm run build-typestyle && node scripts/updateReadme.js",
    "bench-all": "npm run number-bench && npm run puppeteer-bench"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@emotion/react": "^11.1.5",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "css-loader": "^5.0.2",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "lodash.clonedeep": "^4.5.0",
    "node-sass": "^5.0.0",
    "phantom": "^6.3.0",
    "posix": "^4.2.0",
    "puppeteer": "^7.1.0",
    "react-jss": "^10.5.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.1",
    "typestyle": "^2.1.0",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0"
  },
  "keywords": [
    "bechmark",
    "test",
    "css-in-js",
    "react",
    "emotion",
    "sass",
    "typestyle"
  ],
  "author": "Michel Engelen",
  "license": "ISC"
}
